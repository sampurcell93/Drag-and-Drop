:@template"=2/* Welcome to Compass.
 * In this file you should write your main styles. (or centralize your imports)
 * Import this file using the following HTML or equivalent:
 * <link href="/stylesheets/screen.css" media="screen, projection" rel="stylesheet" type="text/css" /> */

@import "compass/reset";
@import "compass/css3";
@import "compass/utilities/general/clearfix";
@import "build";

@font-face {
  font-family: 'icons';
  src: url("../icomoon/fonts/icomoon.eot") format("eot"), 
  url("../icomoon/fonts/icomoon.woff") format("woff"),
  url("../icomoon/fonts/icomoon.ttf") format("truetype");
}

.bs { border: 1px solid red; }
.clickme { 
		color: $strongblue;
		cursor: pointer;
}

* {
	@include box-sizing(border-box);
	margin: 0;
	padding: 0;
}

html { height: 100%; }

body {
	background: url("../images/tweed.png");
	font-family: $opensans;
	text-shadow: 0px 1px 0px #fff;
	color: #444;
	position: relative;
	min-height: 100%;
	padding-top: 40px;

	&:after {
		content: "";
		position: absolute;
		display: none;
		left: 0;
		top: 0px;
		width: 100%; 
		height: 100%;
		background: rgba(0,0,0,.5);
		z-index: 9911;
	}

	&.active-modal::after {
		display: block;
	}

}

h1 { 
	font-size: 2.1em;

}

h2 {
	font-size: 1.6em;

	&.title-bar {
		background: $fadedblue;
		border-bottom: 1px solid $lightblue;
		padding: 5px 15px;
		margin-bottom: 10px;
		width: 95%;
	}
}

h3 { 
	font-size: 1.2em;
	padding: 5px 3px;
}

.tabs {
	height: 40px;
	background: none;
	display: block;
	width: 1200px;
	max-width: 100%;
	margin: 0px auto;


	li {
		display: inline-block;
		height: 100%;
		@include border-radius(2px 2px 0 0);
		line-height: 40px;
		padding: 0 10px;
		background: #333;
		margin: 0 1px;
		color: #f9f9f9;
		text-shadow: 0px -1px 3px #000;
		cursor: pointer;

		&:hover {
			text-decoration: underline;
		}
	}
}

header, footer {
	// needs latest Compass, add '@import "compass"' to your scss
	background-color: #424141; // Old browsers
	@include filter-gradient(#424141, #262626, vertical); // IE6-9
	@include background-image(linear-gradient(top,  #424141 0%,#262626 100%));
	//needs latest Compass, add '@import "compass"' to your scss
	// background-color: #aebcbf; // Old browsers
	// @include filter-gradient(#aebcbf, #0a0809, vertical); // IE6-9
	// @include background-image(linear-gradient(top,  #aebcbf 0%,#6e7774 50%,#0a0e0a 51%,#0a0809 100%));
	@include clearfix();
	@include border-radius(3px 3px 0 0 );
	color: #f9f9f9;
	text-shadow: 0px -1px 0px #111;
	padding: 10px 5px 10px 10px;
	border-bottom: 1px solid #252525;
	border-top: 1px solid #666;

	input { margin-top: 1px;

		&:focus {
			outline: none;
			@include box-shadow(0px 0px 6px 3px $strongblue);
		}
	}
	
	button { 
		font-size: .9em;
		margin: 0 0 0 8px;
	}	

	.view-sections {
		margin: 0 5px 0 15px;
		position: relative;

		&:before {
			content: "";
			position: absolute;
			left: 50%;
			display: none;
			width: 0;
			height: 0;
			border: 10px solid transparent;
			border-top-color: $strongblue;
			top: 100%;
			left: 45%;
		}
		&.active:before { display: block; }
		&:hover::before {border-top-color: #2184A1; }

	}
}

footer {	
	@include border-radius(0 0 3px 3px);
}

.container {
	display: block;
	width: 1200px;
	max-width: 100%;
	margin: 0px auto;
	background: #f9f9f9;
	position: relative;
	@include box-shadow(0px 0px 10px #333);
	@include border-radius(4px);
}

.modal {
	display: block;
	width: 900px;
	position: fixed;
	left: 50%;
	z-index: 9912;
	margin-left: -400px;
	top: 100px;
	background: #fff;
	background: rgba(255,255,255,.89);
	padding: 20px;
	@include box-shadow(0px 0px 7px #222);
	
	.close-modal {
		position: absolute;
		top: 4px;
		right: 4px;
		cursor: pointer;
	}

}

ul.select-properties {
	display: block;
	width: 35%;
	float: left;
	// min-height: 200px;
	position: relative;
	// margin-top: 50px !important;

	.add-property {
		font-size: .88em;
		padding: 0 15px 0 0;
	}

	.hide-properties { cursor: pointer; }

	li{
		display: block;
		min-height: 40px;
		line-height: 40px;
		position: relative;
		padding: 0 !important;
		border-top: 1px solid #fff;
		@include box-shadow(0px -2px 6px #ccc);
		padding-bottom: 10px;

		&:first-child {
			@include border-radius(3px 3px 0 0);
			@include box-shadow(none);
		}
		&:last-child {
			@include border-radius(0 0 3px 3px);
			border-bottom: none;
		}

		&.new-data-type {
			position: absolute;
			top: -40px;
			left: 10px;
			border: none;
			&:after, &:before { display: none; }
		}

		// &:after, &:before {
		// 	content: "";
		// 	display: block;
		// 	width: 50px;
		// 	height: 13px;
		// 	@include box-shadow( 0px -3px 6px #ccc);
		// 	top: -2px;
		// 	@include opacity(.99);
		// 	position: absolute;
		// 	z-index: 0;
		// }
		// &:after { 
		// 	right: 0; 
		// 	// @include transform(rotate(-4deg));
		// }
		// &:before {
		// 	left: 0px;
		// }
	}

	.icon {
		display: inline-block;
		padding: 0 10px;
		text-align: center;	
		@include transition(all .3s ease-in-out);
		color: #999;

		&.flipped {
			@include transform(rotate(180deg));
		}
	}

	.close {
		position: absolute;
		top: 0px;
		right: 10px;
	}

	li ul {
		margin: 0 !important;
	}

	li li, .property {

		@include box-shadow(none);
		text-align: left;
		padding-left: 60px;
		background: #fff; 
		margin: 1px 3px 1px 35px;
		cursor: pointer;
		min-height: 25px;
		line-height: 25px;
		font-size: 1em;
		border-bottom: none;

		&:after, &:before { display: none; }

		&.selected { 
			color: #f9f9f9; 
			background: $strongblue !important;
			text-shadow: none;

			&:hover { color: #fff; }

			.icon { color: #fff; }
		}

		&:hover {
			color: $strongblue;
		}

		&:first-child {
			border-top: 1px solid #ddd;
		}

		&:last-child {
			border-bottom: none;
		}
		&:focus {
			outline: 1px solid #fff;
		}

		&:nth-child(even) {
			background: $fadedblue;
		}
	}

	div[contentEditable] {
		padding-left: 60px;
		&:focus {
			outline: 1px solid #fff;
		}
	}
}

.layout-list {
	li {
		display: inline-block;
		width: 30%;
		padding: 10px;
		margin: 10px 1.5%;
		text-align: center;
		&.selected-layout {
			background: $strongblue;
			color: #fff;
			text-shadow: none;
		}
	}
}

ul.property-editor {
	li {
		padding: 6px 14px 6px 60px;

		&:nth-child(odd) {
			background: $fadedblue;
		}

		.icon {
			font-size: .9em;
			padding: 0 5px;
			color: #555;
			position: relative;

			&:before {
				content: attr(data-tooltip);
				background: rgba(0,0,0,.8);
				position: absolute;
				display: none;
				bottom: 100%;
				left: -50%;
				padding: 4px 6px;
				text-shadow: 0;
				font-family: $opensans; 
				@include border-radius(3px);
				color: #eee;
			}

			&:hover { color: $strongblue; }
			// &:hover::before { display: block; }
		}
	}
}

.sort-element {
	background: $fadedblue;
	font-family: 'icons';
	position: absolute;
	height: 100%;
	width: 30px;
	text-align: center;
	display: block;
	color: #444;
	font-size: .8em;
}

.right-arrow {
	line-height: 300px;
	font-size: 3rem;
	color: $strongblue;
	@include opacity(.3);

	// span:first-of-type {
	// 	-webkit-animation: blink 2s linear infinite alternate;
	// 	-moz-animation: blink 1s linear infinite alternate;
	// 	animation: blink 1s linear infinite alternate;
	// }
	// span:nth-of-type(2) {
	// 	-webkit-animation: blink 2s 4s linear infinite alternate;
	// 	-moz-animation: blink 1s linear infinite alternate;
	// 	animation: blink 1s linear infinite alternate;
	// }
	// span:last-of-type {
	// 	-webkit-animation: blink 2s 8s linear infinite alternate;
	// 	-moz-animation: blink 1s linear infinite alternate;
	// 	animation: blink 1s linear infinite alternate;
	// }
}

@keyframes blink {
	from { @include opacity(1); }
      to { @include opacity(0); }
}
@-moz-keyframes blink {
	from { @include opacity(1); }
      to { @include opacity(0); }
}
@-webkit-keyframes blink {
	from { @include opacity(1); }
      to { @include opacity(0); }
}

ul.generic-elements {
	display: block;
	height: 30px;
	border: none;
	margin: 10px 10px 0 0;
	border: 1px solid #ddd;
	border-bottom: none;
	background: $strongblue;
	color: #fff;
	
	li {
		text-shadow: none;
		display: inline-block;
		line-height: 30px;
		padding: 0px 15px;
		height: 100%;
		margin: 0 3px;
		cursor: pointer;

		&:hover {
			text-decoration: underline;
		}
	}
}


.over {
	// @include transform(scale(.98));
	@include border-radius(3px);
	border: 1px dotted $strongblue !important;
	background: $fadedblue !important;
}

.dragging { 
	opacity: .7;
	z-index: 1111;
}

.moving-sort .sort-element {
	background: $lightblue;
}

#existing-sections {
	position: absolute;
	top: 60px;
	right: -1px;
	width: 182px;
	@include box-shadow(-1px 1px 3px #666);
	background: #555;
	z-index: 111;
	max-height: 80%;
	overflow-y: scroll;

	li {
		// for smoother animations
		background: #555;
		white-space: nowrap;
		text-align: center;
		cursor: move;
		padding: 5px 10px;
		border-top: 1px solid #888;
		border-bottom: 1px solid #333;
		color: #f9f9f9;
		text-shadow: 0px -1px 0px #222;
		max-height: 182px;

		.thumb-object {
			border: 1px solid $medstrongblue;
			padding: 5px;
			margin: 2px;
		}
	}
}

#organize-elements {
	
	min-height: 100%;
	margin: 10px;
	padding: 5px;
	width: 26.5%;
	float: left;
	@include border-radius(3px);
	@include box-shadow(0px 2px 4px #ccc);
	border: 1px solid #ccc;


	li {
		position: relative;
		padding-left: 25px;
		min-height: 30px;
		line-height: 30px;
		display: block;
		cursor: move;
		text-overflow: ellipsis !important;

		&:first-child {
			@include border-radius(2px 2px 0 0);
			.sort-element {
				@include border-radius(2px 0 0 0);
			}
		}
		&:last-child {
			@include border-radius(0 0 2px 2px);
			.sort-element {
				@include border-radius(0 0 0 2px);
			}
			
		}

		&:nth-child(odd) {
			background: $fadedblue;
		}

		.sort-element {
			left: 0px;
			top: 0px;
			cursor: pointer;
			cursor: hand;
			width: 20px;
			text-align: center;
		}

		.child-list { 
			margin-left: -12px !important;
		}

		&.out-of-flow {
			background: #ddd !important;

			.sort-element {background: #ccc; }
		}

		.activate-element, .destroy-element {
			position: absolute;
			right: 10px;
			font-family: 'icons';
			color: #7FBA50;
			cursor: pointer;
		}
		.destroy-element {
			right: 33px;
			color: #666;
		}
	}
}

section.builder-container {
	float: right;
	position: relative;
	margin: 0px 10px 10px;
	padding: 0 5px 40px;
	@include border-radius(3px);
	@include box-shadow(0px 2px 4px #ccc);
	border: 1px solid #ccc;

	.sort-element {
		top: 0;
		right: 0;
	}

	&.no-grid .builder-element {
		border-color: transparent;
	}
}


// Represents a draggable item. Using custom clearfix to avoid overflow hidden problem
.builder-element {
	position: relative;
	margin: 3px 0 3px;
	padding: 4px 10px;
	@include transition(width .2s ease-in-out);
	border: 1px dotted $strongblue;
	@include border-radius(3px);
	background: #fff;
 	*zoom: 1;
 	cursor: move;

 	[contentEditable], [contenteditable] {
		// background: $lightblue;
		// padding: 5px;
		// border: 1px dotted $medstrongblue;
		@extend input;
 	}

 	ul, ol, dl {
 		list-style-position: inside !important;
 		list-style-type: bullet !important;
 		margin-left: 30px;
 	}

 	&:hover { 
		border: 1px solid $strongblue;
		@include box-shadow(0px 0px 4px $medblue);

		> .config-menu-wrap {
			@include opacity(1);
		}
 	}

	&.child { 
		border: 1px dashed $medblue !important;
	}

	input { 
		margin: 10px 0;
		width: 65%;
	}

	&.selected-element {
		border: 1px dashed #FF6100 !important;
	}

	&:before,
	&:after {
    	content: " "; /* 1 */
    	display: table; /* 2 */
	}
	&:after {
    	clear: both;
	}

}

.ui-selectable-helper { 
	position: absolute;
	z-index: 100;
	border:1px dotted $strongblue; 
	background: rgba(125,185,232,.4);
}

.config-menu-wrap {
	@include transition(all .1s ease-in-out);
	@include opacity(0);
	position: absolute;
	top: 0px;
	right: 0px;
	display: block;
	cursor: pointer;
	text-align: center;
	background: #444;
	width: 60px;
	z-index: 999;
	height: 100%;
	max-height: 38px;
	@include border-radius(0 2px 0 2px);

	li {
		@include transition(all .1s ease-in-out);
		display: inline-block;
		text-shadow: none;
		padding: 0 6px;
		width: 50%;
		line-height: 38px;
		color: #f9f9f9;
		position: relative;

		&:hover {
			background: #777;
		}

		li { color: #333; }
	}
}

.set-options {

		.dropdown {
			position: absolute;
			top: 100%;
			right: 1px;
			display: none;
			font-family: $opensans;
			width: 170px;			
			border: 1px solid #bbb;
			@include border-radius(3px);
			@include box-shadow(0px 1px 3px #999);
			z-index: 111;
			background: #fff;
		}

		.dropdown li {
			display: block;
			height: 33px;
			cursor: pointer;
			text-align: center;
			display: block;
			width: 100%;
			line-height: 33px;

			&:hover { 
				background: $strongblue;
				color: #fff;
				text-shadow: none;
			}
		}
	}

button {
	@include border-radius(2px);
